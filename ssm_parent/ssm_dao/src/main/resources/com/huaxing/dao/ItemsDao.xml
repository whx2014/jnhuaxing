<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.huaxing.dao.ItemsDao">
    <select id="findById" parameterType="java.lang.Integer" resultType="com.huaxing.pojo.Items">
        select * from items where id=#{id}
    </select>
</mapper>